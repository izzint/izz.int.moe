---
pagination:
  data: collections
  size: 1
  alias: tag
  filter:
    - all
layout: layouts/base.njk
permalink: /tags/{{ tag }}/
date: git Last Modified
---

<h3 class="header">tagged “{{ tag }}”</h3>

<ol>
{% set taglist = collections[ tag ] %}
{% for post in taglist | reverse %}
  <li><a href="{{ post.url }}">{{ post.data.title }}</a></li>
{% endfor %}
</ol>